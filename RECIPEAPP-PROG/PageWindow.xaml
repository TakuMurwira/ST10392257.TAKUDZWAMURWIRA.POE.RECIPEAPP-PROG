<Window x:Class="RECIPEAPP_PROG.PageWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RECIPEAPP_PROG"
        mc:Ignorable="d"
        Title="PageWindow" Height="450" Width="800">
    <Grid Background="#FF0F1F46">
        <Button x:Name="backBtn" Content="Back" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="48" Click="BackButton_Click"/>
        <Frame x:Name="PageFrame" NavigationUIVisibility="Hidden" Margin="0,50,0,0"/>
    </Grid>
</Window>
